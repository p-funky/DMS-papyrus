{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["./"],
        "includePattern": ".+\\.js$",
        "excludePattern": "(documentation|node_modules|coverage|reports|client|bin|tests)"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "systemName": "Papyrus- Document Management System",
        "copyright": "Pfunky &copy; 2017",
        "navType": "vertical",
        "theme": "flatly",
        "linenums": true,
        "collapseSymbols": true,
        "inverseNav": false,
        "search": true
    },
    "opts": {
        "template": "./node_modules/ink-docstrap/template",
        "encoding": "utf8",
        "destination": "docs",
        "recurse": true,
        "private": true
    }
}